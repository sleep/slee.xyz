{
  "name": "site",
  "scripts": {
    "build": "webpack --config ./webpack.prod.config.js",
    "lint": "eslint .",
    "push": "aws s3 sync --delete ./dist s3://slee.xyz",
    "start": "node server.js",
    "test": "yarn run lint && yarn run test-haskell",
    "test-haskell": "yarn run test-haskell-setup && (cd haskell; stack test)",
    "test-haskell-setup": "rm -f ./haskell/test/Blog/*Spec.lhs ; cd posts ; find . -depth -name \"*.hs.md\" -exec sh -c 'ln \"$1\" \"../haskell/test/Blog/${1%.hs.md}Spec.lhs\"' _ {} \\;"
  },
  "dependencies": {
    "date-fns": "^1.21.1",
    "lodash": "^4.17.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "recompose": "^0.22.0",
    "styled-components": "^1.4.3"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.26.2",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "front-matter": "^2.1.1",
    "highlight.js": "^9.9.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "markdown-it": "^8.2.2",
    "markdown-it-mathjax": "^2.0.0",
    "serve-static": "^1.11.1",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  },
