{
  "name": "site",
  "scripts": {
    "build": "webpack --config ./webpack.production.config.js",
    "lint": "eslint .",
    "push": "aws s3 sync --delete ./dist s3://slee.xyz",
    "start": "node server.js",
    "test": "yarn run lint",
    "test-haskell": "yarn run test-haskell-setup && (cd haskell; stack test)",
    "test-haskell-setup": "rm ./haskell/test/Blog/* ; cd posts ; find . -depth -name \"*.hs.md\" -exec sh -c 'ln \"$1\" \"../haskell/test/Blog/${1%.hs.md}Spec.lhs\"' _ {} \\;"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "date-fns": "^1.21.1",
    "lodash": "^4.17.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "recompose": "^0.21.2"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "front-matter": "^2.1.1",
    "highlight.js": "^9.9.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "markdown-it": "^8.2.2",
    "markdown-it-mathjax": "^2.0.0",
    "serve-static": "^1.11.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.2"
  },
  "author": "Sean Lee <freshdried@gmail.com>",
  "license": "MIT"
}
